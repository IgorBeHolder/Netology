 range(3)]\n","df1 = pd.DataFrame(data)\n","\n","# From a list of dicts with MISSING data\n","df2 = pd.DataFrame([{'a': 1, 'b': 2}, {'b': 3, 'c': 4}])\n","\n","# From a NumPy structured array.\n","A = np.zeros(3, dtype=[('A', 'i8'), ('B', 'f8')])\n","df3 = pd.DataFrame(A)\n","\n","draw_tbl([[pandas_to_html(df1), pandas_to_html(df2), pandas_to_html(df3)]])"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":271},"id":"zus4G5IzM4b4","outputId":"9b68e9ec-ff35-4d80-dd9f-b3dbecb90884"},"outputs":[],"source":["# From a dictionary of Series objects.\n","df3 = pd.DataFrame({'population': population, 'area': area})\n","\n","# From a two-dimensional NumPy array.\n","import numpy as np\n","df4 = pd.DataFrame(np.random.rand(3, 2), columns=['foo', 'bar'], index=['a', 'b', 'c'])\n","\n","draw_tbl([[pandas_to_html(df3), pandas_to_html(df4)]])"]},{"cell_type":"markdown","metadata":{"id":"1_gRl2BANdvu"},"source":["##The Pandas Index Object"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":34},"id":"Q5bfbxLpNfDB","outputId":"747a75ec-b31f-402f-8a93-1d08c4c364b9"},"outputs":[],"source":["ind = pd.Index([2, 3, 5, 7, 11])\n","ind"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":34},"id":"RqOxNqdaPXPk","outputId":"e23766fe-3ff8-464e-d979-81faa315ea87"},"outputs":[],"source":["ind[1]"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":34},"id":"k-UUxqvqPYEq","outputId":"d94f8a6f-38b2-4c23-dbc7-f1202ac4c80b"},"outputs":[],"source":["ind[::2]"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":34},"id":"kvsXLy9_PaDJ","outputId":"1b797fd2-2663-4106-e98d-39da2f7a6bb0"},"outputs":[],"source":["print(ind.size, ind.shape, ind.ndim, ind.dtype)"]},{"cell_type":"code","execution_count":null,"me