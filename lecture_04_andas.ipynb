ount":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":34},"id":"7dKjZ0ztlAoi","outputId":"1c2039bd-9aa2-43fa-8005-277c005dc228"},"outputs":[],"source":["list(data.items())"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":185},"id":"2V4kLmEwlK21","outputId":"89ad6d01-f7d7-4f97-fae9-db8e2cfa8e16"},"outputs":[],"source":["# slicing by explicit index\n","# final index is included in the slice\n","df1 = data['a':'c']\n","\n","# slicing by implicit integer index\n","# final index is excluded from the slice\n","df2 = data[0:2]\n","\n","draw_tbl([[pandas_to_html(df1), pandas_to_html(df2)]])"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":151},"id":"WKkRq9Ymlbb9","outputId":"aeadadd2-8d8b-4f0b-89f1-405fdc0dae42"},"outputs":[],"source":["# masking\n","df1 = data[(data > 0.3) & (data < 0.8)]\n","\n","# fancy indexing\n","df2 = data[['a', 'e']]\n","\n","draw_tbl([[pandas_to_html(df1), pandas_to_html(df2)]])"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":435},"id":"Q140vWfypsJy","outputId":"63161e5a-5db3-45f5-d19e-cfed561be78f"},"outputs":[],"source":["data = pd.Series(['a', 'b', 'c'], index=[1, 3, 5])\n","draw_tbl([['original', 'explicit', 'implicit'],\n","          [{'text': pandas_to_html(data), 'rowspan':4}, 'data.loc[1:3]', 'data[1:3] OR data.iloc[1:3]'],\n","          [ pandas_to_html(data.loc[1:3]), pandas_to_html(data.iloc[1:3])],\n","          ['data[1] OR data.loc[1]', 'data.iloc[1]'],\n","          [str(data.loc[1]), str(data.iloc[1])]])"]},{"cell_type":"markdown","metadata":{"id":"niwRrAs0qc2S"},"source":["##Data Selection in DataFrame"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":342},"id":"R2tjbKnSqeKw","outputId":"5ca59e48-be00-4901-ee96-ac5697c211fa"},"outputs":[],"source":["area = 